## adding predefined values

num1 = 3
num2 = 5
add(num1 num2)

## adding values inside of arguments

add(1 6)

## using the returned value

std_out(add(6 8))

## assigning the return value

variable_name = add(1 6)

std_out(variable_name)

std_out(mult(5 7 4)) ## 5 * 7 * 4

std_out(div(3 5 3)) ## (3 / 5) / 3

std_out(subtr(3 4 6)) ## 3 - 4 - 6

std_out(pow(3 5 6)) ## (3^5)^6